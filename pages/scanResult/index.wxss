/* pages/scanResult/index.wxss */
.container{
  height: 100vh;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.password-tittle, .tips{
  flex: 1;
  width: 100%;
  text-align: center;
  padding: 60rpx 0;
}
.password-content{
  flex: 8;
  font-size: 240rpx;
  font-weight: 900;
  line-height: 240rpx;
}
.tips{
  font-size: 32rpx;
}
.tips .tips-action{
  margin-top: 20rpx;
}
.tips .tips-href{
  color: #b9dd08;
}