/* pages/alert/index.wxss */
.container{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.alert-tittle{
  background-color: #eee;
  padding: 20rpx 0 20rpx 20rpx;
  font-size: 25rpx;
}
.alert-checkBox{
  width: 100%;
}
.alert-checkBox checkbox{
  border: 1px solid rgb(244, 206, 17);
  width: 40%;
  padding: 20rpx 10rpx;
  margin:0 0 10rpx 22rpx;
}
.takePhoto{
  width: 100%;
}
.takePhoto image{
  width: 300rpx;
  height: 300rpx;
  margin: 10rpx;
}
.alert-input{
  width: 100%;
}
.alert-input input{
  margin-left: 20rpx;
  border-bottom: 1px solid #eee;
  height: 40rpx;
  width: 90%;
  
}
.alert-submit{
  width: 100%;
}
.imgselect{
  display: flex;
  flex-flow: row;
}
.imgselect image, .imgselected image{
  margin:auto;
  width: 195rpx;
  height: 195rpx;
}
.imgselected{
  width: 200rpx;
  height: 202rpx;
  text-align: center;
  margin:20rpx 6rpx;
  padding:0px;
  align-items: center;
  justify-content: center;
}